.servicesTitle
    padding 9vw 1vw 0 1vw

.serviceHead
    display grid
    grid-template-rows 1fr
    grid-template-columns repeat(5, 1fr)

.serviceHead1
    display flex
    justify-content center
    grid-row-start 1
    grid-column-start 2
    grid-row-end 2
    grid-column-end 3

.serviceHead2
    display flex
    justify-content center
    grid-row-start 1
    grid-column-start 3
    grid-row-end 2
    grid-column-end 4

.serviceHead3
    display flex
    justify-content center
    grid-row-start 1
    grid-column-start 4
    grid-row-end 2
    grid-column-end 5

.serviceBody
    display grid
    grid-template-rows repeat(5, 1fr)
    grid-template-columns repeat(10, 1fr)

.serviceTitle1
    display flex
    grid-row-start 1
    grid-column-start 3
    grid-row-end 2
    grid-column-end 6

.serviceDiscription1
    display flex
    justify-content flex-end
    grid-row-start 1
    grid-column-start 6
    grid-row-end 2
    grid-column-end 9

.serviceTable1
    display flex
    grid-row-start 2
    grid-column-start 3
    grid-row-end 3
    grid-column-end 9
    border 1px